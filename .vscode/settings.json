{
  "editor.tabSize": 4,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "svelte"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": [
    "HELIUS",
    "lamports",
    "solana"
  ],
}
